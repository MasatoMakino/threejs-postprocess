{
  "name": "threejs-postprocess",
  "discription": "Collection of post process modules for three.js",
  "version": "0.3.0",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "module": "./esm/index.js",
  "files": [
    "lib",
    "esm"
  ],
  "repository": "https://MasatoMakino@github.com/MasatoMakino/threejs-postprocess.git",
  "author": "MasatoMakino <unagiinu.g@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "three": "^0.150.1"
  },
  "devDependencies": {
    "@masatomakino/gulptask-demo-page": "^0.6.0",
    "@types/three": "^0.149.0",
    "browser-sync": "^3.0.2",
    "husky": "^8.0.3",
    "lil-gui": "^0.18.0",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "three": "^0.150.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.26",
    "typescript": "4.9.5"
  },
  "scripts": {
    "start:dev": "npm run server & npm run watch:tsc & npm run watch:demo",
    "build": "npm run buildTS && npm run demo && npm run typedocs",
    "buildTS": "tsc --project tsconfig.json & tsc --project tsconfig.esm.json",
    "watch:tsc": "tsc -W --incremental --project tsconfig.json & tsc -W --incremental --project tsconfig.esm.json",
    "demo": "npx @masatomakino/gulptask-demo-page --body '<canvas id=\"webgl-canvas\" width=\"640\" height=\"480\"></canvas>'",
    "watch:demo": "npm run demo -- -W",
    "typedocs": "npx typedoc --out ./docs/api src/index.ts",
    "server": "browser-sync ./docs/demo -w",
    "prepare": "husky install"
  },
  "dependencies": {
    "@masatomakino/raf-ticker": "0.4.2"
  }
}
